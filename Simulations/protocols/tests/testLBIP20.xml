<worldsens xmlns="http://worldsens.citi.insa-lyon.fr">
<!--
== Worldsens =====================================================
-->
<simulation nodes="20" duration="100s" x="200" y="200"  z="0"/>
<!--
== Entities ======================================================
-->
<!--
== PROPAGATION, INTERFERENCES and MODULATION =====================
-->
<entity name="range" library="propagation_range">
<init range="30"/>
</entity>
<entity name="interf" library="interferences_none"></entity>
<entity name="none" library="modulation_none"></entity>
<!--
== RADIO and ANTENNA ============================================
-->
<entity name="omnidirectionnal" library="antenna_omnidirectionnal">
<default loss="0" angle-xy="random" angle-z="0"/>
</entity>
<entity name="radio" library="radio_half1d">
<default sensibility="-92" T_b="727" dBm="10" channel="0" modulation="none"/>
</entity>
<!--
== MAC =====================================================
-->
<entity name="mac" library="mac_dcf_802_11"></entity>
<!--
== APPLICATION =====================================================
-->

<entity name="app-sensor" library="application_broadcast">
<init period="10s" debut="30s"/>
<default />
</entity>
<!--
== MOBILITY =====================================================
-->
<entity name="static-pos" library="mobility_static">
<default x="random" y="random" z="0.0"/>
</entity>

<!-- == ROUTING ========================================= -->
<entity name="routing-protocole" library="routing_lbip">
<init alpha="1" c="0" eps="0.01" />
<default />
</entity>

<!-- == ENERGY =====================================================
-->
<entity name="theorie" library="energy_theorie">
<default energy="100000000000000" rx="1"  tx="1" debug="0"/>
</entity>

<!--
== Environment =====================================================
-->
<environment>
<propagation entity="range" range="30"/>
<interferences entity="interf"/>
<modulation entity="none"/>
</environment>
<!--
== Bundle =====================================================
-->
<bundle name="sensor" worldsens="false" default="true" birth="0">
<mobility entity="static-pos"/>
<energy entity="theorie" />
<antenna entity="omnidirectionnal">
    <up entity="radio"/>
</antenna>

<with entity="radio">
<up entity="mac"/>
<down entity="omnidirectionnal"/>
</with>

<with entity="mac">
<up entity="routing-protocole"/>
<down entity="radio"/>
</with>

<with entity="routing-protocole">
<up entity="app-sensor"/>
<down entity="mac"/>
</with>

<with entity="app-sensor">
<down entity="routing-protocole"/>
</with>
</bundle>

<!--
== Nodes =====================================================
-->

<node id="0"  >
        <for entity="static-pos" x="40,000000" y="50,000000" z="0.0" />
        <for entity="theorie" energy="100000000" rx="1"  tx="1" debug="0"/>
</node>
